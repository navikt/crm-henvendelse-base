        </rules>
        <rules>
            <name>Automated_Process</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$User.NKS_FullName__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Automated Process</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$User.CRM_NAV_Ident__c</leftValueReference>
                <operator>IsBlank</operator>
