        <locationY>503</locationY>
        <defaultConnectorLabel>Henvendelses-API</defaultConnectorLabel>
        <rules>
            <name>Experience_Site_User</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$User.CRM_NAV_Ident__c</leftValueReference>
